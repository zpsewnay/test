SELECT * FROM SYSTEM_PARAM where PARAM_KEY in ('SQuickloanAmtLimit','QuickloanpdfSaveAP_User','CREDITLINE_CHG_FTP_PORT',
'CREDITLINE_CHG_FTP_TIMEOUT','CREDITLINE_CHG_FTP_HOST','CREDITLINE_CHG_FTP_USERNAME','CREDITLINE_CHG_FTP_PATH','LINE_URL');
select * from DIC where TYPE in ('CAL_AwardCardType', 'CAL_AwardCardType_S','TRM_DAILY_UP','TRM_TOTALDATA_UP','SERVICE','trm_activity_lowpotential',
'callWiiParam','CsLoanInstlmtApplyInqStatus','SQuickLoanParam','SQuickLoanTspIdList','HLOAN_url','Card_OT007_shared_Url','Card_OT007_aem_url_path',
'HolidayRCFlag','TRM_DAILY_UP','QuickLoan_ProjectCode');
select * from DIC_TYPE;
SELECT * FROM TASK WHERE TASK_ID in ('TWRBM_Card_QU035','TWRBO_Invest_QU049','TWRBC_Card_AG026','TWRBM_Card_AG026','TWRBM_Home_QU029','TWRBC_Home_QU029',
'TWRBO_Invest_QU050','TWRBC_Card_OT012');
SELECT * FROM TASK_MAPPING WHERE TASK_ID in ('TWRBC_Card_AG026','TWRBM_Card_AG026');
SELECT * FROM ROLE_TASK;
select * from MENU where MENU_ID in ('TRML2302','TRML060303','TRCL07','TRCL0701','TRCL070101','TRCL0702','TRCL070201','TRCL070202',
'TRCL070203','TRCL070204','TRCL070205','TRCL070206','TRCL0703','TRCL070301','TRML25','TRML2501','TRML2502','TRML250201','TRML250202','TRML250203',
'TRML250204','TRML250205','TRML250206','TRML2503','TRCL040207','TRML0407','TRML97','TRML230201','TRML230202','TRML230203','TRCL00I1','TRCL00I104',
'TRCL00I105','TRCL0704','TRCL070401','TRML2504','TRML00215','TRCL00048','TRCL00120','TRCL04060103','TRCL060206','TRML2304');
SELECT * FROM MENU WHERE TASK_ID = 'TWRBO_card_loan/cashLoanPassword';
select * from I18N WHERE I18N_KEY in ('TRML2302','TWRBM_Card_QU035','TRML060303','TWRBC_Card_AG026','TRCL040207','TWRBM_Card_AG026','TRML0407',
'TWRBC_Card_AG002','TWRBC_Home_QU029','HOMEQU029','TWRBO_Invest_QU050','TWRBC_Card_OT012','TRCL04060103','TRCL060206','TRML2304');
select * from ESB_SERVICE_SETTING;
select * from EXCEPTION_CODE where ERROR_CODE in ('0936','0397','0398','0399','3301','0596','0989','0990','0991','0992','0994','0995','0996','0997','0998','0993',
'I001','I002','I003','I004','I005','I006','I007','I008','I009','I101','I102','I103','I104','I105','I106','I107','I108','I109','I110','I201','I202','I203','I208',
'0002','8888','0999','1910');
select * from ERROR_CODE where ERROR_CODE in  ('9879');
select * from INEB_DIGITAL_ACCT_ACTION_DESC;
select * from HOME_CARD WHERE CARD_ID ='HOMEQU029';
select * from INEB_DB_TOKEN_DATA where OID between '101' and '180' ;
select * from CARD_APPLY_INFO where PITEM between '524' and '527' ;
