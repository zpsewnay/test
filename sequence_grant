
CREATE SEQUENCE PTAPMGRSIT.SEQ_CC_GIFT_COLLECT
MINVALUE 1
MAXVALUE 9999999999
START WITH 1
INCREMENT BY 1
CACHE 20
CYCLE
ORDER;

grant select on PTAPMGRSIT.SEQ_CC_GIFT_COLLECT to EBMWMOD_AP_ROLE_SIT;
grant select on PTAPMGRSIT.SEQ_CC_GIFT_COLLECT to EBMWQRY_AP_ROLE_SIT;

create synonym EBMWUSRSIT01.SEQ_CC_GIFT_COLLECT for PTAPMGRSIT.SEQ_CC_GIFT_COLLECT;
create synonym EBMWUSRSIT02.SEQ_CC_GIFT_COLLECT for PTAPMGRSIT.SEQ_CC_GIFT_COLLECT;
create synonym EBMWUSRSIT03.SEQ_CC_GIFT_COLLECT for PTAPMGRSIT.SEQ_CC_GIFT_COLLECT;
